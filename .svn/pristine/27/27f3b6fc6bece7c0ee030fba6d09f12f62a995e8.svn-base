.myEdit{
		display:block;
		float:left;
		max-width:60px;
		height:18px;
		line-height:15px;
		border:1px solid #efefef;
		border-radius:2px;
		font-size:12px;
		padding:2px 2px 0 24px;
		margin-left:4px;
		background:url(../../res/dwz/themes/default/images/grid/grid.png) no-repeat;
		background-position:  4px -798px;
	}
	.myEdit:hover{
		display:block;
		float:left;
		max-width:60px;
		height:18px;
		line-height:15px;
		border:1px solid #b8d0d6;
		border-radius:2px;
		font-size:12px;
		padding:2px 2px 0 24px;
		margin-left:4px;
		background:url(../../res/dwz/themes/default/images/grid/grid.png) no-repeat;
		background-position: 4px  -798px;
		cursor: pointer;
	}
.bannerModel{
	width:100%;
	height:400px;
		
}
.bannerModel img{
	width:300px;
	height:400px;
	display:block;
	margin:0 auto;
	margin-left:400px;
}