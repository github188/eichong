}
.div-style{
	margin:15px 0px;
}

.div-pay-way{
	height:50px;
	line-height:50px;
}
.div-pay-way .sp{
	float:left;
	margin-right:15px;
}
.div-style .text{
	border:1px solid #bdbdbd;
	width:240px;
	height:28px;
}
.div-style ul li{
	float:left;
	display:inline-block;
	height:40px;
	line-height:40px;
	margin-right:50px;
	cursor:pointer;
}
.div-style span{
    color: #706f6f;
}
.div-style ul li span{
    display: inline-block;
    vertical-align: middle;

}
.div-style ul li .rd{
	margin-right:25px;
	float:left;
	margin-top:15px;
	cursor:pointer;
}
.div-style ul li img{
	cursor:pointer;
}
.div-btn{
    margin-top: 30px;
}
.yellow-bt{
	width:156px;
	height:40px;
	line-height:40px;
	text-align:center;
	color:#fff;
	background:#f08307;
	float:none;
	margin-left:70px;
	display:block;
    border-radius: 5px;
}
