@charset "utf-8";
/* CSS Document */
body, div, ul, li, p, span, h1, h2, h3, h4, h5, h6, img, form, table, tr, td, dl, dt, dd, input, select {
	margin: 0;
	padding: 0;
}
div, ul, li {
	zoom: 1;
}
ul, li, ol {
	list-style: none;
	list-style-type: none;
}
img {
	border: 0 none;
}
a {
	text-decoration: none;
}
a:hover {
	text-decoration: none;
}
h1, h2, h3, h4, h5, h6 {
	font-size: 12px;
	line-height: normal;
	font-style: normal;
	font-weight: normal;
}
.clear {
	margin: 0;
	padding: 0;
	width: 0;
	height: 0;
	line-height: 0;
	font-size: 0;
	list-style: none;
	list-style-type: none;
	clear: both;
	zoom: 1;
}
.q:after {
	content: ".";
	display: block;
	height: 0;
	clear: both;
	font-size: 0px;
	visibility: hidden;
}
.both {
	clear: both;
}
.left {
	DISPLAY: inline;
	FLOAT: left
}
.right {
	DISPLAY: inline;
	FLOAT: right
}
body {background:#fff;}


/*��������--*/
.personalcenter {
	color:#706f6f;
	font-size:14px;
	width:1162px;
	margin:0 auto;
	margin-bottom:45px;
}

.menu{
	width:210px;
	float:left;
	overflow:hidden;
	background:#efece8;
	text-align:center;
	padding:20px 0px;
}
.personalcenter .content{
	width:900px;
	float:right;
	overflow:hidden;
}
.menu h2{
	font-size:18px;
	height:30px;
	line-height:30px;
	border-bottom:1px solid #d4d4d4;
	text-align:left;
	margin-left:20px;
}
.menu-list .line{
	display:block;
	margin-left:20px;
	background:#fcfbf9;	
	height:1px;
}
.menu-list li{
	display:block;
	width:160px;
	height:40px;
	line-height:40px;
	background:url(../images/icon/menu-bt-bg.png) repeat-x;
	text-align:center; 
	margin:0 auto;
	margin-top:8px;
}
.menu-list li a{
	color:#5c5c5c;
}
.menu-list li a.current{
	color:#f08307;	
}
.m_right{
    float: right;
    width: 900px;
    margin: 0 auto;
}
.detail{
    height: 30px;
    line-height: 30px;
    padding: 0 15px;
    background: #f08307;
    text-align: center;
    color: #fff;
    border-radius: 5px;
    margin-left:30px;
    font-size: 16px;
    display: inline-block;
}
.content .person-info{
	height:80px;
	margin-bottom:20px;
}
.person-info .p-img{
	float:left;
}
.person-info .name{
	float:left;
	display:inline-block;
	height:80px;
	line-height:80px;
	margin-left:10px;
}
.info-details .info-title{
	border-bottom:1px solid #d4d4d4;
}
.info-details .info-title{
	height:32px;
	margin-bottom:10px;
	font-size:18px;
}
.info-show .info-details{
	margin-bottom:15px;
}
.info-title .rect{
	float:left;
	display:inline-block;
	width:5px;
	height:15px;
	background:#f08307;
	margin-right:8px;
	margin-top:6px;
}
.info-title h3{
	float:left;
	color:#706f6f;
	font-size:18px;
}
.info-details .info-table{
	width:100%;
	border:0px;
}
.info-table tr td{
	height:40px;
	line-height:40px;
}
.info-table tr td span{
	float:left;
}
.info-table tr td .arrow{
	display:inline-table;
	width:5px;
	height:8px;
	background:url(../images/icon/arrow.png) no-repeat;
	margin:13px 6px 0px 0px;
	float:left;
}
.info-table tr td .bt{
	display:inline-block;
	width:70px;
	height:30px;
	line-height:30px;
	color:#fff;
	text-align:center;
	background:url(../images/icon/orange-bt-bg.png) no-repeat;
}

/*��������--�ҵ�ԤԼ--ԤԼ����*/
.personalappointment .info-details  .info-title{
	height:35px;
	line-height:35px;
}
/*��������--�ҵ�Ǯ��-��������*/
.personalconsume .nav{
	height:41px;
	border-bottom:1px solid #d4d4d4;
	margin-bottom:15px;
}
.personalconsume  .info-details{
	display:none;
}
.content .nav li{
	display:block;
	height:40px;
	line-height:40px;
	padding: 0 20px;
    float:left;
	border:1px solid #d4d4d4;
	border-right:none;
	background:#efece8;
	text-align:center;
	cursor:pointer;
}
.content .nav li.last{
	border-right:1px solid #d4d4d4;
}
.content .nav li.current{
	border-bottom:1px solid #f08307;
	background:#ffffff;
}
.page{
	border-top:1px solid #d4d4d4;
	margin-top:15px;
	padding-top:15px;
	float:right;
	width:100%;
}
.page .sp{
	display:inline-block;
	float:right;
	margin-right:10px;
	height:24px;
	line-height:24px;
	cursor:pointer;
}
.page ul{
	float:right;
}
.page ul li{
	display:inline-block;
	width:24px;
	height:24px;
	line-height:24px;
	text-align:center;
	background:url(../images/icon/circle1-bg.png) no-repeat;
	float:left;
	margin-right:10px;
	cursor:pointer;
}
.page ul li.current{
	background:url(../images/icon/circle2-bg.png) no-repeat;
	color:#fff;
}
.page .next{
	margin-right:0px;
}
/*爱充网-个人中心-充值*/
