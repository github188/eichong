@charset "utf-8";
/* CSS Document */
.page{
    margin-bottom: 40px;
}
.content {
    width: 1162px;
    border-radius: 15px;
    margin: 0px auto;
    overflow: hidden;
}
.list-tit{
    text-indent: 10px;
    height: 60px;
    line-height: 60px;
    font-size: 16px;
    border-top: 1px solid #ccc;
    border-bottom: 1px solid #ccc;
}
.list-tit dt,.list-tit dd{
    float: left;
}
.list-tit dd a{
    color: #333;
    padding: 10px 19px;
    border-radius: 6px;
    font-size: 16px;
    margin-left: 30px;
}
.list-tit dd a.current{
    color: #fff;
    background-color: #f37814;
}
/*****查找电桩内容样式*******/
.search-electric {
    width: 1160px;
    padding-bottom: 20px;
    margin: 10px auto;
    overflow: hidden;
}

.search-electric ul.search-title {
    overflow: hidden;
}

.search-electric ul.search-title li {
    width: 185px;
    height: 35px;
    font-family: "微软雅黑";
    float: left;
    color: #3f3a39;
    font-size: 18px;
    text-align: center;
    line-height: 35px;
    border: 1px solid #dddddd;
    cursor: pointer;
}

.search-electric ul.search-title li.current {
    background-color: #fabf1b;
    color: #fff;
    border: none;
    height: 37px;
    line-height: 37px;
}

.search-electric ul.search-title li img {
    width: 21px;
    vertical-align: middle;
    margin-right: 10px;
}

.search-electric .search-con {
    margin-top: 20px;
    font-family: "微软雅黑";
}

/***第一个内容样式******/
.search-electric .search-con .list-con {
    border: 1px solid #dddddd;
    overflow: hidden;
}

.search-electric .search-con .list-con .list-tit {
    font-size: 14px;
    line-height: 40px;
    text-indent: 10px;
    border-bottom: 1px dashed #dddddd;
    position: relative;

}

.search-electric .search-con .list-con .list-tit img {
    vertical-align: middle;
    margin-right: 10px;

}

.search-electric .search-con .list-con ul {
    padding-left: 40px;
    overflow: hidden;

}

.search-electric .search-con .list-con .area-tit ul {
    display: inline-block;
    overflow: hidden;
    position: absolute;
    top: 5px;
    left: 80px;

}

.search-electric .search-con .list-con .area-tit ul li {
    float: left;
    width: 80px;
    line-height: 30px;
    height: 30px;
    padding: 0 !important;
    border-radius: 8px;
    text-align: left;
    cursor: pointer;
    margin: 0 15px;

}

.search-electric .search-con .list-con .area-tit ul li.area-current {
    background-color: #f37814;
    color: #fff;
    text-align: center;
}

.search-electric .search-con .list-con ul li {
    overflow: hidden;
    font-size: 14px;
    color: #666666;
    padding: 10px 0;
}

.search-electric .search-con .list-con ul li span {
    display: block;
    float: left;
    width: 80px;
    line-height: 30px;
    height: 30px;
    text-align: center;
}

.search-electric .search-con .list-con ul li ul {
    overflow: hidden;
    padding-left: 0;
}

.search-electric .search-con .list-con ul li ul li {
    float: left;
    width: 80px;
    line-height: 30px;
    height: 30px;
    padding: 0;
    border-radius: 8px;
    text-align: center;
    cursor: pointer;
    margin: 0 15px;
}

.search-electric .search-con .list-con ul li ul li.current {
    background-color: #f37814;
    color: #fff;
    text-align: center;
}
.map-con{
    padding-top: 20px;
    position: relative;
    height:870px;
    overflow: hidden;
	display: none;
}
/***第二部分内容样式********/
.electric-num {
    border: 1px solid #dddddd;
    overflow: hidden;
    margin-top:30px;
    padding-bottom: 20px;
}

.electric-num .num-title {
    overflow: hidden;
    background-color: #efece8;

}
.electric-num .num-title li{
    width: 110px;
    height: 50px;
    line-height: 50px;
    text-align: center;
    font-family: "微软雅黑";
    font-size: 18px;
    float: left;
    color: #111111;
    cursor: pointer;
    border-left: 1px solid #dddddd;
    border-right: 1px solid #dddddd;
}
.electric-num .num-title li.num-current{
    background-color: #ff8800;
    border: none;
}
.electric-num .electric-con{
    overflow: hidden;
}
.electric-num .electric-con li{
    font-family: "微软雅黑";
    float: left;
    width: 360px;
    overflow: hidden;
    margin-top:20px;
    margin-left: 18px;
    border: 1px solid #dddddd;
    padding-bottom: 10px;
    float: left;
}
.electric-num .electric-con li img{
    width: 360px;
}
.electric-num .electric-con li h4{
    font-size: 18px;
    font-weight: normal;
    color: #111111;
    text-indent: 13px;
    position: relative;
}
.electric-num .electric-con li h4 .num{
    position: absolute;
    top: 0px;
    right: 5px;
    color: #f18d2c;
}
.electric-num .electric-con li p{
    text-indent: 20px;
    color:#696969;
    font-size: 14px;
}
.electric-num .electric-con .tips-wrap{
    font-size: 0;
    text-indent: 0;
    margin-left: 20px;
}
.tips-wrap .tips{
    vertical-align: middle;
    display: inline-block;
    margin-right: 5px;
    padding:6px 8px;
    background-color: #ddd;
    font-size: 12px;
    color: #333;
    text-align: center;
    border-radius: 5px;
}
.electric-num .electric-con li p img{
    width:12px;
}
/*****查找电桩内容样式结束*******/

/*****电桩内容详情样式*******/
/*{*/
    /*width: 1160px;*/
