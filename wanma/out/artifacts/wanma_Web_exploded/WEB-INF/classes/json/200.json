{
    "title" : {
        "text": "未来一周气温变化",
        "subtext": "纯属虚构"
    },
    "tooltip" : {
        "trigger": "axis"
    },
    "legend": {
        "data":["最高气温","最低气温"]
    },
    "toolbox": {
        "show" : true,
        "feature" : {
            "mark" : {"show": true},
            "dataView" : {"show": true, "readOnly": false},
            "magicType" : {"show": true, "type": ["line", "bar"]},
            "restore" : {"show": true},
            "saveAsImage" : {"show": true}
        }
    },
    "calculable" : true,
    "xAxis" : [
        {
            "type" : "category",
            "boundaryGap" : false,
            "data" : ["周一","周二","周三","周四","周五","周六","周日"]
        }
    ],
    "yAxis" : [
        {
            "type" : "value",
            "axisLabel" : {
                "formatter": "{value} °C"
            }
        }
    ]
}